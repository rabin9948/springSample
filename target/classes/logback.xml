<?xml version="1.0" encoding="UTF-8"?>
<configuration>
	<include resource="org/springframework/boot/logging/logback/base.xml"/>
	<!-- log4jdbc-log4j2 -->
	<logger name="jdbc.sqlonly" level="DEBUG"/>
	<loger name="jdbc.sqltiming" level="INFO"/>
	<loger name="jdbc.audit" level="WARN"/>
	<loger name="jdbc.resultset" level="ERROR"/>
	<loger name="jdbc.resultsettable" level="ERROR"/>
	<loger name="jdbc.connection" level="INFO"/>
</configuration>